<Window x:Class="DraftApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DraftApp"
        mc:Ignorable="d"
        Title="Подсистема для работы со складом и материалами компании Черновик" Height="450" Width="800" Icon="Черновик.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Background="#FEA9AB"></Grid>
        <Image Source="Resources/Черновик.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Черновик" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Grid Grid.Row="2" Background="#FEA9AB"></Grid>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Width="175" Click="BtnBack_Click"></Button>

    </Grid>
</Window>
